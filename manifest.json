{
  "name": "Google CN DevSites",
  "description": "When available, automatically maps Google devsites to its CN mirrors",
  "version": "0.17",
  
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*.google.com/",
    "*://*.android.com/",
    "*://*.angular.io/",
    "*://*.code-labs.io/",
    "*://*.googleblog.com/"
  ],
  
  "background": {
    "persistent" : true,
    "scripts": ["config.js", "bg.js"]
  },
  
  "icons": {
    "128": "on.png"
  },
  
  "browser_action": {},
  
  "manifest_version": 2
}
